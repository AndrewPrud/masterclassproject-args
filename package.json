{
  "name": "code-the-way-project-starter",
  "version": "0.0.1",
  "description": "A project to help jump-start Code The Way UI development.",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --env production",
    "clean": "rm -rf docs/ && echo \"\\x1b[102m\\x1b[1m CLEAN \\x1b[0m\" docs directory removed",
    "eslint": "./node_modules/.bin/eslint src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" No JavaScript ESLint errors to fix",
    "eslint:fix": "./node_modules/.bin/eslint --fix src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" ESLint JavaScript errors fixed",
    "pre-publish": "npm run --silent stylelint && npm run --silent eslint && npm run --silent clean && npm run --silent build",
    "start": "./node_modules/.bin/webpack serve --env development",
    "stylelint": "./node_modules/.bin/stylelint 'src/**/*.scss' --config stylelint.config.js && echo \"\\x1b[102m\\x1b[1m STYLELINT \\x1b[0m\" No StyleLint SCSS errors to fix",
    "stylelint:fix": "npm run stylelint -- --fix && echo \"\\x1b[102m\\x1b[1m STYLELINT \\x1b[0m\" StyleLint SCSS errors fixed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeyschroeder/code-the-way-project-starter.git"
  },
  "author": "Joey Schroeder <schrjos@gmail.com>",
  "license": "ISC",
  "pre-commit": {
    "run": [
      "eslint",
      "stylelint"
    ],
    "silent": true
  },
  "bugs": {
    "url": "https://github.com/joeyschroeder/code-the-way-project-starter/issues"
  },
  "homepage": "https://github.com/joeyschroeder/code-the-way-project-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.7",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "buffer": "6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^9.2.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "path": "^0.12.7",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "postcss-scss": "^4.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "process": "0.11.10",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hot-loader/react-dom": "^17.0.2",
    "@mui/icons-material": "^5.6.1",
    "@mui/material": "^5.6.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.8",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.1"
  }
